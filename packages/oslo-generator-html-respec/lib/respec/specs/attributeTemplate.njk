<h2>{{attribute.label}}</h2>


<table>
  <tr>
    <th>Type</th>
    <td>Attribute</td>
  </tr>
  <tr>
    <th>URI</th>
    <td>{{attribute.id | urlize() | safe}}</td>
  </tr>


  <tr>
    <th>Definition</th>
    {% if attribute.definition %}
    <td>{{attribute.definition}}</td>
    {% else %}
    <td style="color:red;">Empty</td>
  </tr>
{% endif %}


  <tr>
    <th>UsageNote</th>
    {% if attribute.usageNote %}
    <td>{{attribute.usageNote}}</td>
    {% else %}
    <td style="color:red;">Empty</td>
  </tr>
{% endif %}

  <tr>
    <th>Domain</th>
    {% if attribute.domain %}
    <td>{{attribute.domain | urlize() | safe}}</td>
    {% else %}
    <td style="color:red;">Empty</td>
  </tr>
{% endif %}

  <tr>
    <th>Range</th>
    {% if attribute.range %}
    <td>{{attribute.range}}</td>
    {% else %}
    <td style="color:red;">Empty</td>
  </tr>
{% endif %}

  <tr>
    <th>Scope</th>
    {% if attribute.scope %}
    <td>{{attribute.scope| urlize() | safe}}</td>
    {% else %}
    <td style="color:red;">Empty</td>
  </tr>
{% endif %}

  <tr>
    <th>UsageNote</th>
    {% if attribute.usageNote %}
    <td>{{attribute.usageNote}}</td>
    {% else %}
    <td style="color:red;">Empty</td>
  </tr>
{% endif %}

  <tr>
    <th>Parent</th>
    {% if class.parents %}
    <td>
    {% for parent in class.parents%}
      {{parent.id.value}}
    {% endfor %}
    </td>
    {% else %}
    <td style="color:red;">Empty</td>
  </tr>
{% endif %}

  <tr>
    <th>Parent Label</th>
    {% if class.parents %}
    <td>
    {% for parent in class.parents%}
      {{parent.label.value}}
    {% endfor %}
    </td>
    {% else %}
    <td style="color:red;">Empty</td>
  </tr>
{% endif %}

</table>